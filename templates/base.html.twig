<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Precious Uzoaru{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
        {% endblock %}
        {% block fontawesome %}
            <script src="https://kit.fontawesome.com/f78be57a4b.js" crossorigin="anonymous"></script>
        {% endblock fontawesome %}
    </head>
    <body>

    <nav class="navbar navbar-expand-lg navbar-light bg-warning">
        <a class="navbar-brand text-dark font-weight-bolder" href="#{{ path('Home') }}">Precious UZOARU</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav ml-auto text-center">
                <li class="nav-item active">
                    <a class="nav-link text-dark font-weight-bolder pr-5" href="{{ path('Home') }}">Home</a>
                </li>

                <li class="nav-item active">
                    <a class="nav-link text-dark font-weight-bolder pr-5" href="{{ path('Apropos.index') }}">Apropos</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link text-dark font-weight-bolder pr-5" href=" {{ path('competence.index') }}">Competence</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-dark font-weight-bolder pr-5" href="{{ path('Realisation.index') }}">Realisations</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link text-dark font-weight-bolder pr-5" href="{{ path('app_login') }}">Login</a>
                </li>
            </ul>
        </div>
    </nav>
        {% block body %}
        {% endblock %}


        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
            <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>

        {% endblock %}
    </body>
</html>
